{
  "name": "painel-chamada-realtime",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/server/**/*.ts' --exec \"ts-node --project tsconfig.server.json server.ts\"",
    "dev:debug": "nodemon --watch src --watch server.ts --ext ts --exec \"tsx server.ts --inspect=0.0.0.0:24678\"",
    "dev:api": "cross-env NEXT_ENABLED=false nodemon --watch src --watch server.ts --ext ts --exec \"tsx server.ts\"",
    "dev:api:debug": "cross-env NEXT_ENABLED=false nodemon --watch src --watch server.ts --ext ts --exec \"tsx --inspect=0.0.0.0:24678 server.ts\"",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:server && npm run build:next-if-enabled",
    "build:api": "npm run build:server",
    "build:next-if-enabled": "if [ \"$NEXT_ENABLED\" = \"true\" ]; then npm run build:next; else echo \"NEXT_ENABLED=false â†’ pulando build do Next.js\"; fi",
    "start": "NODE_ENV=production node dist/server.js",
    "start:api": "cross-env NODE_ENV=production NEXT_ENABLED=false node dist/server.js",
    "build:test": "tsc scripts/test-broadcast.ts --outDir dist-test --target es2020 --module commonjs --esModuleInterop --resolveJsonModule --skipLibCheck",
    "test:broadcast": "node dist-test/test-broadcast.js",
    "test": "jest --config jest.config.ts --runInBand",
    "lint": "next lint || true"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "mysql2": "^3.15.3",
    "next": "^16.0.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^5.0.10",
    "inflight": "^1.0.6"
  }
}